class MinStack:
    def __init__(self):
        self.s=[]
        self.m=[]
    def push(self, x):
        self.s.append(x)
        self.m.append(x if not self.m or x<=self.m[-1] else self.m[-1])
    def pop(self):
        self.s.pop();self.m.pop()
    def top(self):
        return self.s[-1]
    def getMin(self):
        return self.m[-1]
